#include <stdio.h>
int factorial (int number){
    if(number==1 || number==0){
        return 1;
    }
    else{
        return (number*factorial(number-1));
    }
}
int main (){

    int n;
    printf("Enter the number ou want factorial of : \n ");
    scanf("%d",&n);
    printf("the factorial of %d is %d \n ",n,factorial(n));
}
